<p id="notice"><%= notice %></p>

<h1>Admin Rates</h1>

<table>
  <thead>
    <tr>
      <th>Rate</th>
      <th>Finished at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_rates.each do |admin_rate| %>
      <tr>
        <td><%= admin_rate.rate %></td>
        <td><%= admin_rate.finished_at %></td>
        <td><%= link_to 'Show', admin_rate %></td>
        <td><%= link_to 'Edit', edit_admin_rate_path(admin_rate) %></td>
        <td><%= link_to 'Destroy', admin_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Rate', new_admin_rate_path %>
